package first_project;

public class TestaReferencias {

	public static void main(String[] args) {
		// Aprendendo a trabalhar com referencias
		
		Conta c1 = new Conta();
		c1.deposita(100);
		
		Conta c2 = new Conta();
		c2.deposita(200);
		
		System.out.println(c1.saldo);
		System.out.println(c2.saldo);
		
	}

}
